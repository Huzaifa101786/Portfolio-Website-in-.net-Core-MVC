@using static System.Net.Mime.MediaTypeNames;
@model List<Portfolio_website.Models.Portfolio_Data>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
@foreach (var data in Model)
{
<h1 class="title text-center">@data.Proj_Name</h1>


 
<div class="slideshow-container">

    <!-- Full-width images with number and caption text -->
    <div class="mySlides fade">
        <div class="numbertext">1 / 3</div>
        <img style="height:550px; width:100%" src="@data.Pic_1" style="width:100%">
        <div class="text">Image 1</div>
    </div>

    <div class="mySlides fade">
        <div class="numbertext">2 / 3</div>
        <img style="height:550px; width:100%" src="https://images.pexels.com/photos/2387418/pexels-photo-2387418.jpeg?auto=compress&cs=tinysrgb&w=600" style="width:100%">
        <div class="text">Image 2</div>
    </div>

    <div class="mySlides fade">
        <div class="numbertext">3 / 3</div>
        <img style="height:550px; width:100%" src="https://images.pexels.com/photos/640809/pexels-photo-640809.jpeg?auto=compress&cs=tinysrgb&w=600" style="width:100%">
        <div class="text">Image 3</div>
    </div>

    <!-- Next and previous buttons -->
    <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
    <a class="next" onclick="plusSlides(1)">&#10095;</a>
</div>
<br>

<!-- The dots/circles -->
<div style="text-align:center">
    <span class="dot" onclick="currentSlide(1)"></span>
    <span class="dot" onclick="currentSlide(2)"></span>
    <span class="dot" onclick="currentSlide(3)"></span>
</div>




<h1 class="title text-center">PROJECT details</h1>
<div class="row">
    <div class="col-md-6" style="height: 450px">
        <h3 class="title text-center">DEMO VIDEO</h3>
        <iframe width="100%"
                height="315"
                type="video/mp4"
                @*https://player.vimeo.com/external/269971860.sd.mp4?s=a3036bd1a9f15c1b31daedad98c06a3b24cdd747&profile_id=164&oauth2_token_id=57447761*@
                src="@data.video";
                frameborder="0"
                allowfullscreen>
        </iframe>
    </div>
    <div class="col-md-6" @*id="abt_me"*@>
        <h3 class="title text-center">PROJECT TYPE</h3>
        <label class="col-8 offset-2 text-center" id="lbl_txt"><b>@data.Proj_Type</b></label>
        <h3 class="title text-center pt-5">Languages Used</h3>  
        <label class="col-8 offset-2 text-center" id="lbl_txt"><b> @data.Proj_Language  @*C# , Html , CSS , JS , JQuery*@</b></label>
    </div>
</div>
<h2 class="title text-center pt-5">PROJECT Description</h2>
<div class="row">
    <textarea class="col-8 text-center row-8 offset-2" id="lbl_txt2" cols="30" ; rows="9">@data.Proj_Description</textarea>
    <div class="col-2"></div>
    <input type="image" src="data:image/png;base64," name="submit" style="width:20px ;height:20px" alt="submit" />
</div>
    break;
}
    
    

}